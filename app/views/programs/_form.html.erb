<% form_for @program, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %>: 
    <%= f.text_field :title %>
  </p>
  <p>
  	<%= f.label :url %>: 
  	<%= root_url %><%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :logo %>: 
    <%= f.file_field :logo %>
  </p>
  <p>
  	<%= f.label :start %>: 
  	<%= f.date_select :start %> | 
	<%= f.label :end %>: 
  	<%= f.date_select :end %>
  </p>
  <p>
  	<%= f.label :description %>:<br />
  	<%= f.text_area :description, :rows => 5, :cols => 100 %>
  </p>
  <p>
    <%= f.label :guidlines %>:<br />
    <%= f.text_area :guidlines, :rows => 5, :cols => 100 %>
  </p>
  <p>
    <%= f.label :contact %>:<br />
    <%= f.text_area :contact, :rows => 5, :cols => 100 %>
  </p>

  <p><%= f.submit %></p>
<% end %>
