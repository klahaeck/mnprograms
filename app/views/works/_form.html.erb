<% form_for @work do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :submission_id %><br />
    <%= f.text_field :submission_id %>
  </p>
  <p>
    <%= f.label :type_id %><br />
    <%= f.text_field :type_id %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p><%= f.submit %></p>
<% end %>
